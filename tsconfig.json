{
  "compilerOptions": {
    "target": "ES6",
    "module": "CommonJS",
    "outDir": "./src/compile",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.ts"
  ]
}
